# ubuntu command
alias cx="clear"
alias qq="exit"
alias remove="rm -rf"
alias google="ping 8.8.8.8"

# GIT
alias git="g"

# laravel command
alias art="php artisan"
alias serve="php artisan serve"
alias tinker="php artisan tinker"
alias reload="art cache:clear && art view:clear && art optimize && art serve --host=javastudio.dev"

# VIM Config
alias vimconf="vim ~/.vimrc"

# php
alias php:serve="php -S 127.0.0.1:8000 -t public"

# go to direktorie
alias z="cd ~/sandbox"
alias desk="cd ~/Desktop"
alias dj="cd ~/sandbox/project/javastudio.id"

# Apache Server
alias restart:apache="sudo service apache2 restart"
alias restart:mysql="sudo service mysql restart"
