#!/bin/bash

_password_length="$1"

if [ -z "$_password_length" ]; then
	_password_length="64"
fi

# LC_CTYPE=C tr -dc '[:graph:]' </dev/urandom | head -c265 | tr -d ' \n' | cut -c 1-"$_password_length"
LC_CTYPE=C tr -dc '[:print:]' </dev/urandom | head -c265 | tr -d ' \n' | cut -c 1-"$_password_length"
