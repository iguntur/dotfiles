#!/bin/bash

DOT_FILES="$HOME/.dotfiles/autoload"
DOT_EXTRA="$HOME/.dotfiles/extra"

## secret key
load_source "$DOT_EXTRA/secret"

## environment PATH
load_source "$DOT_FILES/workspace"
load_source "$DOT_EXTRA/workspace"

## function
load_source "$DOT_FILES/function"
load_source "$DOT_EXTRA/function"

## aliases
load_source "$DOT_FILES/aliases"
load_source "$DOT_EXTRA/aliases"

## misc
load_source "$DOT_EXTRA/utils"
load_source "$DOT_EXTRA/misc"
